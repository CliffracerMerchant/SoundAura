<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="32dp"
    android:height="32dp"
    android:viewportWidth="32"
    android:viewportHeight="32">
    <aapt:attr name="android:drawable">
        <vector
            android:width="32dp"
            android:height="32dp"
            android:viewportWidth="32"
            android:viewportHeight="32">
            <path android:name="path"
                android:pathData="M 11 8 L 25 16 L 11 24 Z"
                android:fillColor="#FF000000"/>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom="M 11,8 L 11,16 L 25,16 L 25,16 L 11,8 M 11,24 L 11,16 L 25,16 L 25,16 L 11,24 Z"
                android:valueTo="M 22,8 L 18,8 L 18,24 L 22,24 L 22,8 M 10,8 L 14,8 L 14,24 L 10,24 L 10,8 Z"
                android:valueType="pathType"
                android:duration="250" />
        </aapt:attr>
    </target>

</animated-vector>